{
  "workbench.colorTheme": "Dracula Theme",
  "security.workspace.trust.untrustedFiles": "open",
  "workbench.iconTheme": "vscode-icons",
  "git.autofetch": true,
  "eslint.nodeEnv": "",
  "redhat.telemetry.enabled": true,
  "editor.stickyScroll.enabled": true,
  "editor.renderWhitespace": "all",
  "editor.formatOnType": true,
  "editor.fontSize": 16,
  "editor.tabSize": 2,
  "editor.guides.indentation": true,
  "workbench.list.multiSelectModifier": "alt",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "eslint.workingDirectories": [
    {
      "mode": "auto"
    }
  ],
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "git.openRepositoryInParentFolders": "always",
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "files.associations": {
    ".eslintrc.cjs": "javascript",
    "*.*proj": "xml",
    "*.props": "xml",
    "*.targets": "xml",
    "*.tasks": "xml"
  },
  "editor.formatOnSave": false,
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit",
    "source.fixAll.eslint": "explicit"
  },
  "eslint.validate": [
    "vue",
    "javascript",
    "javascriptreact",
    "html"
  ],
  "emmet.includeLanguages": {
    "aspnetcorerazor": "html",
    "razor": "html",
    "*.cshtml": "html"
  },
  "razor.devmode": true,
  "dotnet.defaultSolution": "store.sln",
  "cSpell.words": [
    "additionalfields",
    "adipisci",
    "amet",
    "apiauth",
    "apigw",
    "Appbar",
    "appsettings",
    "ASPNETCORE",
    "AVERYTOPSECRET",
    "bien",
    "Blazor",
    "Blazored",
    "Bson",
    "chinh",
    "clean",
    "consectetur",
    "creditcard",
    "Datetime",
    "dolorem",
    "Dtos",
    "equalto",
    "fileextensions",
    "hinh",
    "Hmac",
    "maxlength",
    "Mediat",
    "mimtype",
    "mongoadmin",
    "Mudblazor",
    "Neque",
    "Newtonsoft",
    "ngang",
    "nonalphamin",
    "porro",
    "quisquam",
    "rangelength",
    "Serilog",
    "Serilogger",
    "swaggerui",
    "tedu",
    "thuc",
    "v'VVV",
    "valmsg",
    "velit",
    "weatherforecast"
  ],
  "dotnet.preferRuntimeFromSDK": true,
  "editor.linkedEditing": true
}